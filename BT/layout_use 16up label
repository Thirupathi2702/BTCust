{
    public class LabelDataStructureStandard
    {
        static List<StandardLayout> _labelLayouts = null;
        static List<LayoutContainer> _containers = null;
        static LabelDataStructureStandard()
        {
            _labelLayouts = new List<StandardLayout>();
            _labelLayouts.Clear();
            _containers = new List<LayoutContainer>();
            _containers.Clear();
            InitializeContainers();

        }
        public static List<StandardLayout> GetAllLayouts()
        {
            _labelLayouts.Clear();
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "16up",
                Height = 588,
                Width = 375,
                PaperHeight = 588,
                PaperWidth = 375,
                Containers = GetContainers("16up"),

            });

            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "20up",
                Height = 590,
                Width = 375,
                PaperHeight = 590,
                PaperWidth = 375,
                Containers = GetContainers("20up"),
            });

            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "3up",
                Height = 175,
                Width = 375,
                PaperHeight = 175,
                PaperWidth = 375,
                Containers = GetContainers("3up")
            });

            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "12up",
                Height = 475,
                Width = 375,
                PaperHeight = 475,
                PaperWidth = 375,
                Containers = GetContainers("12up")
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = PrintDataConstants.PrintCategory.CLSSEPARATOR,
                Height = 587,
                Width = 375,
                PaperHeight = 587,
                PaperWidth = 375,
                Containers = GetContainers(PrintDataConstants.PrintCategory.CLSSEPARATOR)
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = PrintDataConstants.PrintCategory.JBSEPARATOR,
                Height = 587,
                Width = 375,
                PaperHeight = 587,
                PaperWidth = 375,
                Containers = GetContainers(PrintDataConstants.PrintCategory.JBSEPARATOR)
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = PrintDataConstants.PrintCategory.DMPSEPARATOR,
                Height = 850,
                Width = 1100,
                PaperHeight = 850,
                PaperWidth = 1100,
                Containers = GetContainers(PrintDataConstants.PrintCategory.DMPSEPARATOR)
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = PrintDataConstants.PrintCategory.DONUT1UPSEPARATOR,
                Height = 250,
                Width = 187,
                PaperHeight = 250,
                PaperWidth = 187,
                Containers = GetContainers(PrintDataConstants.PrintCategory.DONUT1UPSEPARATOR)
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = PrintDataConstants.PrintCategory.DONUT2UPSEPARATOR,
                Height = 250,
                Width = 375,
                PaperHeight = 250,
                PaperWidth = 375,
