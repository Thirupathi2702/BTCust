            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "Donut1up",
                Height = 250,
                Width = 187,
                PaperHeight = 250,
                PaperWidth = 187,
                Containers = GetContainers("Donut1upWBC"),
                MetaInfo = "Donut1up"
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "Donut1up",
                Height = 160,
                Width = 160,
                PaperHeight = 160,
                PaperWidth = 160,
                Containers = GetContainers("Donut1upNoBC"),
                MetaInfo = "NoBC"
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "Donut1up",
                Height = 160,
                Width = 160,
                PaperHeight = 160,
                PaperWidth = 160,
                Containers = GetContainers("Donut1upLabelObselute"),
                MetaInfo = "OBSOL"
            });
            _labelLayouts.Add(new StandardLayout
            {
                LayoutType = "Donut2up",
                Height = 250,
                Width = 375,
                PaperHeight = 250,
                PaperWidth = 375,
                Containers = GetContainers("Donut2upNoBC"),
                MetaInfo = "Donut2up"
            });
            return _labelLayouts;
        }

        private static List<LayoutContainer> GetContainers(string layoutType)
        {
            return _containers.Where(container => container.Id.ToLower().Contains(layoutType.ToLower())).ToList();
        }

